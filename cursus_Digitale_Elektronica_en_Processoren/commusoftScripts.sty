\newcommand{\combinGate}[7]{
\def\dx{0.35}
\def\dy{0.7}
\def\ny{0.5}%We are the knights who say Ni!
\def\noy{0.25}
\def\sc{0.65}
\begin{scope}[xshift=#1 cm, yshift=#2 cm]
\setcounter{tmpC}{0}
\foreach \l/\t/\ty in {#4} {
  \coordinate (V\l) at (\dx*\arabic{tmpC},0);
  \draw[anchor=north] (V\l) node {\t};
  \ifthenelse{\ty=2}{
    \node[not gate,scale=\sc,rotate=90] (NV\l) at (\dx*\arabic{tmpC},\ny);
    \coordinate (Vn\l) at (V\l);
    \draw (V\l) -- (NV\l.input);
    \draw (NV\l.output) -- (V\l |- 0,#3 cm);
  }{\draw (V\l) -- (V\l |- 0,#3 cm);
    \ifthenelse{\ty=3}{
    \addtocounter{tmpC}{1}
    \coordinate (Vn\l) at (\dx*\arabic{tmpC},0);
    \node[not gate,scale=\sc,rotate=90] (NV\l) at (\dx*\arabic{tmpC},\ny);
    \draw (V\l |- 0,\noy) -| (NV\l.input);
    \pdot{V\l |- 0,\noy}
    \draw (NV\l.output) -- (NV\l.output |- 0,#3 cm);
    }{}
  }
  \addtocounter{tmpC}{1}
}
\addtocounter{tmpC}{1}
\setcounter{tmpD}{0}
\setcounter{tmpF}{1}
\foreach \l/\i/\k in {#5} {
  \addtocounter{tmpF}{1}
}
\foreach \l/\i/\k in {#5} {
  \addtocounter{tmpD}{1}
  \node[and gate,inputs=\i,scale=\sc] (A\l) at (\dx*\arabic{tmpC},2*\ny+\arabic{tmpD}*#3/\arabic{tmpF}-\arabic{tmpD}*2*\ny/\arabic{tmpF});
  \setcounter{tmpE}{1}
  \foreach\v in \k {
    \draw (A\l.input \arabic{tmpE}) -- (A\l.input \arabic{tmpE} -| V\v);
    \pdot{A\l.input \arabic{tmpE} -| V\v}
    \addtocounter{tmpE}{1}
  }
}
\addtocounter{tmpC}{1}
\node[or gate,inputs=#6,scale=\sc] (O) at (\dx*\arabic{tmpC}+\dx,\ny+0.5*#3);
